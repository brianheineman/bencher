---
export const prerender = false;

import { BENCHER_API_URL } from "astro:env/server";

import BaseLayout from "../../../../layouts/BaseLayout.astro";
import CheckoutRedirect from "../../../../components/console/redirects/CheckoutRedirect";

const { organization } = Astro.params;
const title = "Bencher Checkout";
---

<BaseLayout title={title} description={title}>
  <div slot="navbar"></div>
  <CheckoutRedirect client:only="solid-js" apiUrl={BENCHER_API_URL} organization={organization} />
  <div slot="footer"></div>
</BaseLayout>