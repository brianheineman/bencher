---
export const prerender = false;

import { BENCHER_API_URL } from "astro:env/server";

import OrgLayout from "../../../../../layouts/console/OrgLayout.astro";
import PosterPanel from "../../../../../components/console/poster/PosterPanel";
import { BencherResource } from "../../../../../config/types";

const params = Astro.params;
const title = "Add Project";
---

<OrgLayout apiUrl={BENCHER_API_URL} params={params} title={title}>
  <PosterPanel
    client:only="solid-js"
    apiUrl={BENCHER_API_URL}
    params={params}
    resource={BencherResource.PROJECTS}
  />
</OrgLayout>
