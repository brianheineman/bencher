---
export const prerender = true;

import LearnPanel from "../../../components/learn/LearnPanel.astro";
import { otherLanguages } from "../../../i18n/ui";
export async function getStaticPaths() {
	return otherLanguages.map((lang) => {
		return { params: { lang } };
	});
}
const { lang } = Astro.params;
---

<LearnPanel lang={lang}/>
